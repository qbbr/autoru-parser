{
    "name": "qbbr/autoru-parser",
    "description": "auto.ru site parser",
    "type": "project",
    "authors": [
        {
            "name": "Sokolov Innokenty",
            "email": "imqbbr@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.3.11"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "optimize-autoloader":  true
    },
    "autoload": {
        "psr-4": {
            "QBBR\\": "src/"
        }
    },
    "require": {
        "php": "^7.3",
        "fzaninotto/faker": "^1.9",
        "phpoffice/phpspreadsheet": "^1.9",
        "qbbr/tor-proxy": "^1.0",
        "symfony/console": "^4.3",
        "symfony/css-selector": "^4.3",
        "symfony/dom-crawler": "^4.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3"
    }
}
